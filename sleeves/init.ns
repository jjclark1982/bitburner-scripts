export async function main(ns) {
    for (let i = 0; i < ns.sleeve.getNumSleeves(); i++) {
        const stats = ns.sleeve.getSleeveStats(i);
        if (stats.shock > 20) {
            ns.sleeve.setToShockRecovery(i);
            continue;
        }
        if (stats.sync < 100) {
            ns.sleeve.setToSynchronize(i);
            continue;
        }
        ns.sleeve.setToCommitCrime(i, "Mug");   
    }
}